#compdef git-import-branch

# Note: This method is inspired by the __git_flow_feature_list method found in
# the git flow completion script.
# It will only return branches starting with feature/ or review/
_git-import-branch () {
	declare -a branches

	branches=(${${(f)"$(_call_program import-branch-list git branch --list --no-color 'feature\*' 'review\*' 'bugfix\*' 2> /dev/null | tr -d ' |*')"}})
	__git_command_successful || return

  compadd "$@" $branches
}

_git-import-branch "$@"


